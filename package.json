{
  "name": "fp-3",
  "version": "1.0.0",
  "description": "A compact, practical functional programming toolkit for TypeScript",
  "main": "dist/allTS.js",
  "types": "dist/allTS.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsc --project tsconfig.run-examples.json",
    "typecheck": "tsc --project tsconfig.typecheck.json",
    "typecheck:prod": "tsc --project tsconfig.prod.json --noEmit",
    "typecheck:prod:runnable": "tsc --project tsconfig.prod-runnable.json --noEmit",
    "examples": "node dist/run-runnable-examples.js",
    "examples:runnable": "ts-node --project tsconfig.runnable-plan.json run-runnable-examples.ts",
    "examples:runnable:coverage": "node scripts/run-runnable-coverage.mjs",
    "test": "vitest run --silent --reporter=dot --bail=10",
    "test:laws": "vitest test/laws",
    "test:ci": "npm run check:stubs && vitest run test/laws --reporter=verbose",
    "test:nightly": "npm run check:stubs && vitest run test/laws --reporter=verbose --run",
    "test:coverage": "node scripts/run-vitest-coverage.mjs",
    "test:coverage:ci": "npm run check:stubs && node scripts/run-vitest-coverage.mjs --reporter=verbose",
    "test:ui": "vitest --ui",
    "test:new": "vitest test/sum-product.spec.ts test/two-cat.registry-and-laws.spec.ts test/two-cat.traversable-laws.spec.ts test/comonad.pair.coalgebra.spec.ts test/comonad.store.spec.ts test/cokleisli.laws.spec.ts test/simplicial.comonad.pair.spec.ts test/homology.pair.spec.ts test/homology.store.spec.ts test/glue.records.spec.ts test/mixed-distributive-laws.spec.ts --run",
    "coverage": "node test/laws/coverage-matrix.js",
    "coverage:ci": "node test/laws/coverage-matrix.js && npm run test:ci",
    "precommit": "node scripts/derived-law-lint.mjs && npm run typecheck:prod && npm run test:new",
    "audit:any": "node scripts/audit-any-usage.mjs",
    "fix:any": "node scripts/any-fix-suggestions.mjs",
    "demo:any": "node scripts/demo-any-fixes.mjs",
    "upgrade:analyze": "node pdi/upgrade-analyzer.js analyze",
    "upgrade:backlog": "node pdi/upgrade-analyzer.js backlog",
    "upgrade:stats": "node pdi/upgrade-analyzer.js stats",
    "upgrade:reference": "node pdi/upgrade-analyzer.js reference",
    "maintenance:check": "node pdi/maintenance-reminder.js check",
    "maintenance:remind": "node pdi/maintenance-reminder.js remind",
    "tasks:check": "node pdi/task-scheduler.js check",
    "tasks:list": "node pdi/task-scheduler.js list",
    "tasks:increment": "node pdi/task-scheduler.js increment",
    "docs:check": "node pdi/docs-checker.js check",
    "docs:stale": "node pdi/docs-checker.js stale",
    "docs:update": "node pdi/docs-checker.js update",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "laws": "tsx laws.monoidal.ts",
    "gen:stubs": "ts-node --compiler-options '{\"module\":\"CommonJS\",\"verbatimModuleSyntax\":false}' scripts/gen-oracle-stubs.ts",
    "check:stubs": "ts-node --compiler-options '{\"module\":\"CommonJS\",\"verbatimModuleSyntax\":false}' scripts/gen-oracle-stubs.ts --dry",
    "validate-relative-monads": "ts-node --project tsconfig.runnable-plan.json scripts/validate-relative-monads.ts",
    "generate:virtual-equipment": "node scripts/generate-virtual-equipment.mjs",
    "prepare": "node scripts/setup-git-hooks.cjs"
  },
  "keywords": [
    "functional-programming",
    "typescript",
    "fp",
    "monads",
    "option",
    "result",
    "recursion-schemes"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.4.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@typescript-eslint/utils": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.0.0",
    "fast-check": "^4.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
